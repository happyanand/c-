import java.util.Scanner;
class A
{
    public static void main(String args[])
    {
        Scanner A=new Scanner(System.in);
        int n=A.nextInt();
        for(int i=1;i<=n;i++)
        {
            for(int j=1;j<=n;j++)
            {
                if(i>=j)
                System.out.print("*");
            }
            System.out.println();
        }
    }
}















